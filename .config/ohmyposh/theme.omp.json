{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "type": "session",
          "style": "diamond",
          "background": "#2899F5",
          "foreground": "#E6EEF6",
          "leading_diamond": "\ue0b6",
          "template": " {{ .UserName }} ",
          "trailing_diamond": "\ue0b0"
        },
        {
          "type": "path",
          "style": "powerline",
          "background": "#1FE4C2",
          "foreground": "#071521",
          "powerline_symbol": "\ue0b0",
          "options": {
            "folder_separator_icon": " \ue0b1 ",
            "home_icon": "~",
            "style": "folder"
          },
          "template": " \uea83  {{ .Path }} "
        },
        {
          "type": "git",
          "style": "powerline",
          "background": "#4DE6FF",
          "foreground": "#071521",
          "leading_diamond": "\ue0b6",
          "powerline_symbol": "\ue0b0",
          "trailing_diamond": "\ue0b4",
          "options": {
            "branch_template": "{{ trunc 25 .Branch }}",
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FFC857{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#9B7BFF{{ end }}",
            "{{ if gt .Ahead 0 }}#2B4255{{ end }}",
            "{{ if gt .Behind 0 }}#FF5C7A{{ end }}"
          ],
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} "
        },
        {
          "type": "node",
          "style": "powerline",
          "background": "#2B4255",
          "foreground": "#E6EEF6",
          "powerline_symbol": "\ue0b0",
          "options": {
            "fetch_version": true
          },
          "template": " \ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} "
        },
        {
          "type": "go",
          "style": "powerline",
          "background": "#4DE6FF",
          "foreground": "#071521",
          "powerline_symbol": "\ue0b0",
          "options": {
            "fetch_version": true
          },
          "template": " \ue626 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} "
        },
        {
          "type": "julia",
          "style": "powerline",
          "background": "#2899F5",
          "foreground": "#E6EEF6",
          "powerline_symbol": "\ue0b0",
          "options": {
            "fetch_version": true
          },
          "template": " \ue624 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} "
        },
        {
          "type": "python",
          "style": "powerline",
          "background": "#A9B6C3",
          "foreground": "#071521",
          "powerline_symbol": "\ue0b0",
          "options": {
            "display_mode": "files",
            "fetch_virtual_env": false
          },
          "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} "
        },
        {
          "type": "ruby",
          "style": "powerline",
          "background": "#071521",
          "foreground": "#A9B6C3",
          "powerline_symbol": "\ue0b0",
          "options": {
            "display_mode": "files",
            "fetch_version": true
          },
          "template": " \ue791 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} "
        },
        {
          "type": "azfunc",
          "style": "powerline",
          "background": "#1FE4C2",
          "foreground": "#071521",
          "powerline_symbol": "\ue0b0",
          "options": {
            "display_mode": "files",
            "fetch_version": false
          },
          "template": " \uf0e7{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} "
        },
        {
          "type": "aws",
          "style": "powerline",
          "foreground": "#E6EEF6",
          "powerline_symbol": "\ue0b0",
          "options": {
            "display_default": false
          },
          "background_templates": [
            "{{if contains \"default\" .Profile}}#2899F5{{end}}",
            "{{if contains \"jan\" .Profile}}#1FE4C2{{end}}",
            "#2B4255"
          ],
          "template": " \ue7ad {{ .Profile }}{{ if .Region }}@{{ .Region }}{{ end }} "
        },
        {
          "type": "root",
          "style": "powerline",
          "background": "#FFC857",
          "foreground": "#071521",
          "powerline_symbol": "\ue0b0",
          "template": " \uf0ad "
        },
        {
          "type": "executiontime",
          "style": "plain",
          "background": "#2B4255",
          "foreground": "#E6EEF6",
          "options": {
            "always_enabled": true
          },
          "template": "<transparent>\ue0b0</> \ueba2 {{ .FormattedMs }}\u2800"
        },
        {
          "type": "status",
          "style": "diamond",
          "background": "#071521",
          "foreground": "#A9B6C3",
          "powerline_symbol": "\ue0b0",
          "trailing_diamond": "\ue0b4",
          "options": {
            "always_enabled": true
          },
          "background_templates": [
            "{{ if gt .Code 0 }}#FF5C7A{{ end }}"
          ],
          "template": "<parentBackground>\ue0b0</> \ue23a "
        }
      ],
      "type": "prompt"
    },
    {
      "segments": [
        {
          "type": "shell",
          "style": "plain",
          "background": "#2899F5",
          "foreground": "#E6EEF6",
          "template": "<#2899F5,transparent>\ue0b6</> \uf489 {{ .Name }} <transparent,#2899F5>\ue0b2</>"
        },
        {
          "type": "ytm",
          "style": "powerline",
          "background": "#1FE4C2",
          "foreground": "#071521",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "options": {
            "paused_icon": "\uf04c ",
            "playing_icon": "\uf04b "
          },
          "template": " \uf167 {{ .Icon }}{{ if ne .Status \"stopped\" }}{{ .Artist }} - {{ .Track }}{{ end }} "
        },
        {
          "type": "battery",
          "style": "powerline",
          "background": "#A9B6C3",
          "foreground": "#071521",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "options": {
            "charged_icon": "\ue22f ",
            "charging_icon": "\ue234 ",
            "discharging_icon": "\ue231 "
          },
          "background_templates": [
            "{{if eq \"Charging\" .State.String}}#4DE6FF{{end}}",
            "{{if eq \"Discharging\" .State.String}}#2B4255{{end}}",
            "{{if eq \"Full\" .State.String}}#1FE4C2{{end}}"
          ],
          "template": " {{ if not .Error }}{{ .Icon }}{{ .Percentage }}{{ end }}{{ .Error }}\uf295 "
        },
        {
          "type": "time",
          "style": "diamond",
          "background": "#071521",
          "foreground": "#A9B6C3",
          "invert_powerline": true,
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "\ue0b4",
          "template": " {{ .CurrentDate | date .Format }} "
        }
      ],
      "type": "rprompt"
    }
  ],
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "final_space": true,
  "version": 4
}
